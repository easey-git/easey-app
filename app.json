{
  "expo": {
    "name": "Easey CRM",
    "slug": "easey-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logo/easey.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow Easey CRM to use FaceID for secure login."
      }
    },
    "android": {
      "icon": "./logo/easey.jpg",
      "package": "com.easey.crm",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "194232ae-d544-4928-92bf-6cd52d54e1e5"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/194232ae-d544-4928-92bf-6cd52d54e1e5"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/sounds/live.mp3"
          ]
        }
      ],
      "expo-secure-store"
    ]
  }
}