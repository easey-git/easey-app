{
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/proxy/delhivery-internal/(.*)",
            "destination": "https://ucp-app-gateway.delhivery.com/web/api/$1"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ]
}